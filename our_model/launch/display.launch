<launch>
  <arg
    name="our_model" />
  <arg
    name="gui"
    default="true" />
  <param
    name="robot_description"
    textfile="$(find our_model)/robots/OUR-II.URDF" />
  <param
    name="use_gui"
    value="$(arg gui)" />
  <node
    name="joint_state_publisher"
    pkg="joint_state_publisher"
    type="joint_state_publisher" />
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find our_model)/urdf.rviz" />
</launch>
